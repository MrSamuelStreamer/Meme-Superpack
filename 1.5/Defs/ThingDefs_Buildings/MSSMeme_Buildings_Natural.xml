<?xml version="1.0" encoding="utf-8"?>

<Defs>

	<ThingDef ParentName="RockBase">
		<defName>MSSMeme_MineableCoalOne</defName>
		<label>coal</label>
		<description>Compact coal ready to be mined. No I will not elaborate.</description>
		<graphicData>
			<texPath>Things/Building/Linked/RockFlecked_Atlas</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<shaderType>CutoutComplex</shaderType>
			<color>(90,90,90)</color>
			<colorTwo>(50,50,50)</colorTwo>
		</graphicData>
		<statBases>
			<MaxHitPoints>1000</MaxHitPoints>
		</statBases>
		<building>
			<isResourceRock>true</isResourceRock>
			<mineableThing>MSSMeme_Coal</mineableThing>
			<mineableYield>25</mineableYield>
			<mineableScatterCommonality>.2</mineableScatterCommonality>
			<mineableScatterLumpSizeRange>15~20</mineableScatterLumpSizeRange>
		</building>
	</ThingDef>

