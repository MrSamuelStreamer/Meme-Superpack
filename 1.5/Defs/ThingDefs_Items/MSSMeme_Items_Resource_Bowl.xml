<?xml version="1.0" encoding="utf-8"?>

<Defs>

	<ThingCategoryDef>
		<defName>MSSMeme_CoalBowls</defName>
		<label>coal bowls</label>
		<parent>Manufactured</parent>
	</ThingCategoryDef>

	<!-- Bowl smelting recipe -->

	<RecipeDef>
		<defName>MSSMeme_SmeltCoalBowls</defName>
		<label>smelt coal bowls</label>
		<description>Use an extreme flame to try desperately to get rid of these damn bowls and get something useful, anything will do!</description>
		<jobString>Sticking a bowl in the fire.</jobString>
		<workAmount>1600</workAmount>
		<workSpeedStat>SmeltingSpeed</workSpeedStat>
		<effectWorking>Smelt</effectWorking>
		<soundWorking>Recipe_Smelt</soundWorking>
		<specialProducts>
			<li>Smelted</li>
		</specialProducts>
		<ingredients>
			<li>
				<filter>
					<categories>
						<li>MSSMeme_CoalBowls</li>
					</categories>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<categories>
				<li>MSSMeme_CoalBowls</li>
			</categories>
		</fixedIngredientFilter>
	</RecipeDef>

	<!-- Bowls -->

	<ThingDef ParentName="ResourceBase">
		<defName>MSSMeme_BowlOne</defName>
		<label>coal bowl</label>
		<description>A bowl made of coal. You could smelt it to get a small amount of whatever coal this bowl is made from, or you could scatter it somewhere. It is a nice looking bowl after all. Who knows, it may even have a use.</description>
		<possessionCount>1</possessionCount>
		<graphicData>
			<texPath>Things/Item/Resource/MSSMeme_Bowl</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<color>(60,60,60)</color>
		</graphicData>
		<stackLimit>1</stackLimit>
		<healthAffectsPrice>false</healthAffectsPrice>
		<intricate>true</intricate> <!-- Not returned to player when product made of this is smelted. -->
		<statBases>
			<MaxHitPoints>20</MaxHitPoints>
			<MarketValue>420</MarketValue>
			<Mass>0.1</Mass>
			<Beauty>8</Beauty>
			<Flammability>2.0</Flammability>
		</statBases>
		<recipeMaker>
			<researchPrerequisite>MSSMeme_SuperCoalResearch</researchPrerequisite>
			<skillRequirements>
				<Crafting>8</Crafting>
			</skillRequirements>
			<recipeUsers Inherit="False">
				<li>TableSculpting</li>
			</recipeUsers>
		</recipeMaker>
		<costList>
			<MSSMeme_Coal>169</MSSMeme_Coal>
		</costList>
		<thingCategories>
			<li>MSSMeme_CoalBowls</li>
		</thingCategories>
		<smeltable>true</smeltable>
		<smeltProducts>
			<MSSMeme_Coal>75</MSSMeme_Coal>
		</smeltProducts>
	</ThingDef>

